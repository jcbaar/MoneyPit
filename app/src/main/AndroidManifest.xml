<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.development.jaba.moneypit" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.development.jaba.moneypit.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.development.jaba.moneypit.AndroidDatabaseManager" />
        <activity
            android:name="com.development.jaba.moneypit.AddOrEditCarActivity"
            android:label="@string/title_activity_add_or_edit_car">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.development.jaba.moneypit.MainActivity" />
        </activity>
    </application>

</manifest>
